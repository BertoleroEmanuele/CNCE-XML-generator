						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_Sesso">
				<xs:annotation>
					<xs:documentation>Sesso del lavoratore</xs:documentation>
				</xs:annotation>
