	<xs:group name="RapportoDiLavoro">
		<xs:annotation>
			<xs:documentation>Dati identificativi del rapporto di lavoro</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LV_DataAssunzione">
				<xs:annotation>
					<xs:documentation>Indicare la data di inizio del rapporto di lavoro con l'impresa </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:union memberTypes="xs:date">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:union>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_TipoAssunzione">
				<xs:annotation>
					<xs:documentation>Tipologia del rapporto di lavoro (vedi tabella per i valori ammessi)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_TipoOccupazione">
				<xs:annotation>
					<xs:documentation>Indicare se il rapporto di lavoro è full-time o part-time. Assume i valori –P se Part Time ed –F se a tempo pieno</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="1"/>
						<xs:enumeration value="P"/>
						<xs:enumeration value="F"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_EsenzLimitiCCNL">
				<xs:annotation>
					<xs:documentation>Nel caso di occupazione Part Time (LV_TipoOccupazione = P), inserire il tipo di esenzione prevista dal contratto per la regolarità del rapporto di lavoro (vedi tabella in allegato per i valori ammessi) </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_SpecificaRapporto">
				<xs:annotation>
					<xs:documentation>Indica eventuali casi particolari del rapporto di lavoro, come da tabella in allegato</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_GiornoCessazione">
				<xs:annotation>
					<xs:documentation>Indicare il giorno, del mese cui si riferisce la denuncia della cessazione del rapporto di iscrizione del lavoratore alla Cassa Edile.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_TipoCessazione">
				<xs:annotation>
					<xs:documentation>Tipo di causa della cessazione del rapporto di lavoro di iscrizione del lavoratore alla Cassa Edile (vedi tabella CNCE per i valori ammessi)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_CodiceQualifica">
				<xs:annotation>
					<xs:documentation>Indicare il codice relativo al livello di inquadramento del lavoratore (vedi tabella CNCE per i valori ammessi, Specifica Tecnica, Allegato 3)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_CodiceSemestreApprendistato" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Campo Facoltativo: Codice del Semestre di Apprendistato ove previsto dal CCNL (esempio Accordo Industria Cooperazione Aprile 2019 semestri da 1S a 8S)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_CodiceMansione">
				<xs:annotation>
					<xs:documentation>Indicare il codice relativo alla mansione svolta dal lavoratore (vedi tabella CNCE per i valori ammessi Specifica Tecnica, Allegato 3)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_OreSettimanali" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Indicare le effettive ore lavorative contrattuali settimanali, se diverse da 40 (es. lavoratori Part Time, Discontinui, etc.); in tutti gli altri casi il campo assume valore = 40.  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LV_PercPartTime">
				<xs:annotation>
					<xs:documentation>Per i lavoratori Part Time (LV_TipoOccupazione = P) indicare la percentuale di Part Time.  In tutti gli altri casi impostare il campo al valore 0</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="5"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
