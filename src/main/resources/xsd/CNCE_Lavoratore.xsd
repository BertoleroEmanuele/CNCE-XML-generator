				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="2"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PREP_AnnoA">
				<xs:annotation>
					<xs:documentation>Anno finale di competenza del versamento pregresso</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PREP_MeseA">
				<xs:annotation>
					<xs:documentation>Mese finale di competenza: il periodo finale PREP_AnnoA + PREP_MeseA deve essere minore del periodo di competenza della denuncia;</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="2"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PREP_ContribLavoratore">
				<xs:annotation>
					<xs:documentation>Importo contributo obbligatorio a carico del lavoratore. Indicare la somma dei contributi maturati nel periodo indicato</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PREP_ContribVolontario">
				<xs:annotation>
					<xs:documentation>Importo contributo facoltativo a carico del lavoratore. Indicare la somma dei contributi maturati nel periodo indicato</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PREP_ContribDitta">
				<xs:annotation>
					<xs:documentation>Importo contributo a carico della ditta. Indicare la somma dei contributi maturati nel periodo indicato</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PREP_ContribContrattuale">
				<xs:annotation>
					<xs:documentation>Importo contributo Contrattuuale a carico della ditta. Indicare la somma dei contributi maturati nel periodo indicato, valido per periodi dal 2015</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="6"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PREP_QuotaTFR">
				<xs:annotation>
					<xs:documentation>Importo contributo detratto dal TFR. Indicare la somma dei contributi maturati nel periodo indicato</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PREP_Rivalutazione">
				<xs:annotation>
					<xs:documentation>Importo della rivalutazione effettuata sulla quota TFR per effetto del decreto ex art. 1 comma 765 legge 296/06 del 30 Gennaio 2007,ovvero altri indennizzi richiesti dalla Cassa Edile per il ritardato versamento del contributo</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="DatiIstitutoTFR">
		<xs:annotation>
			<xs:documentation>Informazioni relative alle Casse Edili che gestiscono l'istituto del TFR</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TFR_TFRDaVersare">
				<xs:annotation>
					<xs:documentation>Inserire l’importo della quota TFR da versare alla Cassa Edile</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TFR_OreFigurative">
				<xs:annotation>
					<xs:documentation>Inserire le ore per donazione sangue e eventuali ulteriori ore con versamento volontario della quota mensile (servizio militare, aspettativa, periodo di malattia oltre 270° giorno di assenza)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TFR_GiorniLavorabili">
				<xs:annotation>
					<xs:documentation>Inserire la somma delle ore esposte escluse quelle per ferie, assenza giustificata, permessi non retribuiti, festività, diviso le ore lavorabili al giorno, con arrotondamento per eccesso.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="2"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TFR_ImportoFAP">
				<xs:annotation>
					<xs:documentation>Importo del contributo versato anticipatamente da parte dell'impresa all'INPS in base alla legge 297/1982 art. 3, 15° comma e calcolato sull'imponibile INPS. In particolare attualmente tale importo è pari allo 0,50% dell’imponibile INPS e va indicato in tutti i casi in cui è effettivamente anticipato dall’azienda.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TFR_ImportoAumDim">
				<xs:annotation>
					<xs:documentation>Importo correttivo per la retribuzione utile per il TFR (esempio: festività 4 novembre, maggiorazione elettricisti o altro)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TFR_SegnoImportoAD">
				<xs:annotation>
					<xs:documentation>A=Importo del campo TFRImportiAurmDim è da considerare in aumento per la retribuzione utile per il TFR; D=Importo del campo TFRImportiAurmDim è da considerare in diminuzione per la retribuzione utile per il TFR</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TFR_OreUtili">
				<xs:annotation>
					<xs:documentation>Ore Utili ai fini del calcolo della Retribuzione Utile TFR (Imponibile TFR)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TFR_PagaOraria">
				<xs:annotation>
					<xs:documentation>Paga Oraria applicata alle Ore Utili TFR ai fini del calcolo della Retribuzione Utile TFR (Imponibile TFR)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="7"/>
						<xs:fractionDigits value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="ORE_CassaIntegrazione">
		<xs:annotation>
			<xs:documentation>Dettaglio delle ore di Cassa Integrazione</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ORE_CIG">
				<xs:annotation>
					<xs:documentation>Indicare il numero totale delle ore di assenza per Cassa Integrazione Guadagni.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORECIG_Maltempo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicare il numero delle ore di assenza per CIG nel mese dovute ad avverse condizioni metereologiche. Il campo deve essere compilato solo in caso di lavoratore apprendista. In caso di compilazione può essere indicato l'eventuale importo richiesto a rimborso LV_ImportoRimborsoCIG</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORECIG_Altro" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicare il numero delle ore di assenza per CIG nel mese dovute a motivi diversi dalle avverse condizioni metereologiche. Il campo deve essere compilato solo in caso di lavoratore apprendista. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORECIG_Ordinaria" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORECIG_Straordinaria" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORECIG_Deroga" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="ORE_Ferie">
		<xs:annotation>
			<xs:documentation>Dettaglio delle ore di ferie</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ORE_Ferie">
				<xs:annotation>
					<xs:documentation>Indicare il numero totale delle ore di assenza per ferie godute nel mese</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OREFE_FerieAC" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicare il numero delle ore di assenza per ferie maturate nell'anno in corso, godute nel mese</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OREFE_FerieResidueAP" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicare il numero delle ore di assenza per ferie residue anno precedente godute nel mese</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="ORE_PermessiRetributi">
		<xs:annotation>
			<xs:documentation>Dettaglio delle ore di permesso retribuito</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OREAG_PermRetrib" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Totale Ore di permesso retribuito (88 ore) </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OREAG_PermRetribAC" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Totale Ore di permesso retribuito (88 ore) maturate nell'anno in corso, godute nel mese</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OREAG_PermRetribAP" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Totale Ore di permesso retribuito (88 ore) residue dell'anno precedente, godute nel mese</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="ORE_Sanzionabili">
		<xs:annotation>
			<xs:documentation>AI sensi della circolare CNCE 373 del 11 dicembre 2008</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ORESA_TotaleOreSanz">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORESA_FerieSanzionabili" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ore di ferie,eccedenti i limiti contrattualmente previsti (160 ore nell'anno solare) e dichiarate nel mese corrente, ai quali si applica la contribuzione dovuta alla Cassa Edile, esclusi gli accantonamenti ai sensi della circolare CNCE 373 del 11 dicembre 2008.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORESA_PermNonRetrSanz" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ore di peremsso non retribuito,eccedenti i limiti contrattualmente previsti (40 ore nell'anno solare) e dichiarate nel mese corrente, ai quali si applica la contribuzione dovuta alla Cassa Edile, esclusi gli accantonamenti ai sensi della circolare CNCE 373 del 11 dicembre 2008.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ORESA_PermRetrSanz" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ore di permesso retribuito,eccedenti i limiti contrattualmente previsti (88 ore nell'anno solare) e dichiarate nel mese corrente, ai quali si applica la contribuzione dovuta alla Cassa Edile, esclusi gli accantonamenti ai sensi della circolare CNCE 373 del 11 dicembre 2008.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="3"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="Imponibili">
		<xs:annotation>
			<xs:documentation>Elementi imponibili del lavoratore</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LV_ImponibileGNF">
				<xs:annotation>
					<xs:documentation>Indicare l'importo totale della retribuzione del lavoratore (compresi eventuali superminimi) su cui è calcolato l'accantonamento di gratifica natalizia e ferie. Deve essere pari o superiore alla somma degli imponibili contributivi relativi ai singoli cantieri (LC_ImponibileGNF). L'importo va arrotondato all'unità di euro</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_ImponibileContributivo">
				<xs:annotation>
					<xs:documentation>Indicare l'importo totale della retribuzione del lavoratore su cui sono calcolati i contributi dovuti alla Cassa Edile. Se presente deve essere pari o superiore alla somma degli imponibili contributivi relativi ai singoli cantieri (LC_ImponibileContributivo). L'importo va arrotondato all'unità di euro.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_ImponibileContrSanz">
				<xs:annotation>
					<xs:documentation>Indicare l'importo totale per ore sanzionabili dichiarate per il lavoratore, sul quale viene calcolato l'importo della sanzone. L'importo va arrotondato all'unità di euro. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LV_ImponibileTFR">
				<xs:annotation>
					<xs:documentation>Indicare l'importo di retribuzione imponibile del lavoratore valevole ai fini del calcolo del trattamento da fine rapporto di lavoro L'importo va arrotandato all'unità di euro.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="5"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TFR_ImponibileINPS">
				<xs:annotation>
					<xs:documentation>Inserire la retribuzione complessiva imponibile ai fini INPS del lavoratore</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="5"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="Accantonamenti">
		<xs:annotation>
			<xs:documentation>Importi di accantonamento del lavoratore</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LV_AccantonGNF">
				<xs:annotation>
					<xs:documentation>Indicare l'importo di accantonamento per gratifica natalizia e ferie relativo alle ore di lavoro prestate.  Deve essere pari o superiore alla somma degli accantonamenti relativi a ciascun cantiere (LC_AccantonGNF).e va arrotondato all'unità di euro.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_AccantonGNFMalInf">
				<xs:annotation>
					<xs:documentation>Indicare l'importo di accantonamento per gratifica natalizia e ferie relativo alle ore di assenza per malattia, infortunio o malattia professionale, congedo per maternità / paternità.
L'importo va arrotondato all'unità di euro.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_AccantonRiposiAnnui">
				<xs:annotation>
					<xs:documentation>Importo accantonato per riposi annui (88 ore) secondo eventuali accordi a carattere locale</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_AccantonFerie">
				<xs:annotation>
					<xs:documentation>Importo accantonato per ferie secondo eventuali accordi a carattere locale</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_AnticipoAccantonamento">
				<xs:annotation>
					<xs:documentation>Importo eventualmente anticipato all’azienda per permessi retribuiti, secondo eventuali accordi a carattere locale</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_AltroAccantonamento" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Eventuale ulteriore importo accantonato presso la Cassa Edile, sulla base di specifici accordi territoriali. L’utilizzo di questo campo deve essere espressamente concordato con le singole Casse Edili interessate.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="6"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_GGOreAltroAccantonamento" minOccurs="0">
				<xs:annotation>
