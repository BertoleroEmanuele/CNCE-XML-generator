					<xs:documentation>Data di nascita del lavoratore</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:union memberTypes="xs:date">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:union>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_CodComunediNascita">
				<xs:annotation>
					<xs:documentation>Codice Catastale del comune di nascita del lavoratore</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_ComunediNascita">
				<xs:annotation>
					<xs:documentation>Denominazione del comune di nascita del lavoratore; è obbligatorio compilare il campo anche se si è indicato il valore del campo LV_CodComunediNascita</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:whiteSpace value="preserve"/>
						<xs:minLength value="0"/>
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LV_ProvinciadiNascita">
